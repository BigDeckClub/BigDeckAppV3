    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.22",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "fast-check": "^3.0.0",
    "globals": "^15.15.0",
    "jsdom": "^24.0.0",
    "postcss": "^8.4.47",
    "supertest": "^6.3.3",
    "tailwindcss": "^3.4.18",
    "terser": "^5.44.1",
    "typescript": "^5.6.4",
    "vite": "^5.0.0",
