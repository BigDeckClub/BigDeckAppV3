{
  "name": "bigdeck-app",
  "version": "1.0.0",
  "type": "module",
  "description": "BigDeck.app - MTG Inventory Tracker using React and Vite",
  "scripts": {
    "build:autobuy": "tsc -p tsconfig.autobuy.json",
    "autobuy": "npm run build:autobuy && node bin/run-autobuy.mjs",
    "autobuy:run": "npm run build:autobuy && node bin/run-autobuy.mjs",
    "autobuy:verify": "npm run build:autobuy && node scripts/verify-autobuy.cjs",
    "dev": "npm run build && npm run build:autobuy && node server.js",
    "build": "vite build",
    "gcp-build": "npm run build",
    "preview": "vite preview",
    "start": "node server.js",
    "prod": "npm run build && node server.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/ server/",
    "lint:fix": "eslint src/ server/ --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.0",
